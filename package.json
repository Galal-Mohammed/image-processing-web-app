{
  "name": "image-processing-api",
  "version": "1.0.0",
  "description": "This is an image processing web application that allows users to resize and upload images. The app consists of a backend API built with Express.js and a frontend user interface implemented using HTML, CSS, and JavaScript.",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project ./ && npm run copy:assets",
    "start": "nodemon src/index.ts",
    "test": "ts-node node_modules/jasmine/bin/jasmine.js --config=jasmine.json",
    "lint": "eslint --ext .ts src",
    "prettier": "prettier --config .prettierrc --write \"src/**/*.ts\"",
    "copy:assets": "cpx \"src/images//*\" build/images"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "image-size": "^1.2.1",
    "multer": "^2.0.1",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jasmine": "^5.1.8",
    "@types/multer": "1.4.11",
    "@types/node": "^20.11.30",
    "@types/sharp": "0.31.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "cpx": "^1.2.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jasmine": "^5.8.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dhadhazi/Image-Processing-Web-App.git"
  },
  "bugs": {
    "url": "https://github.com/Dhadhazi/Image-Processing-Web-App/issues"
  },
  "homepage": "https://github.com/Dhadhazi/Image-Processing-Web-App#readme"
}
